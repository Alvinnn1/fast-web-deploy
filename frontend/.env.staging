# Staging Environment Variables for Frontend
NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://luckyjingwen.top

# Cloudflare Configuration
VITE_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Application Configuration
VITE_APP_TITLE=Cloudflare Static Deployer (Staging)
VITE_APP_VERSION=1.0.0-staging

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_PRELOAD=true

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://staging.yourdomain.com