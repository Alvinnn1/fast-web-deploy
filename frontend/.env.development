# Development Environment Variables for Frontend
NODE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000

# Cloudflare Configuration (for local development)
VITE_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Application Configuration
VITE_APP_TITLE=Cloudflare Static Deployer (Dev)
VITE_APP_VERSION=1.0.0-dev

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=2000
VITE_ENABLE_PRELOAD=false

# Security Configuration
VITE_ENABLE_CSP=false
VITE_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000