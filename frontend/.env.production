# Production Environment Variables for Frontend
NODE_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.luckyjingwen.top

# Cloudflare Configuration
VITE_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
VITE_CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id_here

# Application Configuration
VITE_APP_TITLE=Cloudflare Static Deployer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=A powerful tool for managing Cloudflare static website deployments

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=500
VITE_ENABLE_PRELOAD=true
VITE_ENABLE_PREFETCH=true
VITE_ENABLE_COMPRESSION=true

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
VITE_ENABLE_HTTPS_ONLY=true

# Cloudflare Pages Static Deployment
VITE_DEPLOYMENT_TYPE=static
VITE_PAGES_DEPLOYMENT_ID=
VITE_PAGES_PROJECT_NAME=cloudflare-static-deployer
VITE_PAGES_ENVIRONMENT=production
VITE_WORKERS_ENABLED=false

# Build Optimizations
VITE_ENABLE_TREE_SHAKING=true
VITE_ENABLE_CODE_SPLITTING=true
VITE_ENABLE_MINIFICATION=true
VITE_TARGET_BROWSERS=defaults and not IE 11

# CDN and Caching
VITE_CDN_URL=
VITE_CACHE_STRATEGY=aggressive
VITE_STATIC_ASSETS_TTL=31536000

# Monitoring and Logging
VITE_LOG_LEVEL=error
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=