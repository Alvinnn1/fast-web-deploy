{
  "name": "cloudflare-static-deployer-backend",
  "version": "1.0.0",
  "description": "Backend API for Cloudflare static deployer",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:workers": "tsc --project tsconfig.workers.json",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "postinstall": "npm run build",
    "wrangler:dev": "wrangler dev",
    "wrangler:deploy": "wrangler deploy",
    "wrangler:tail": "wrangler tail",
    "workers:validate": "node scripts/validate-workers-config.js"
  },
  "dependencies": {
    "fastify": "^4.24.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.22.0",
    "@cloudflare/workers-types": "^4.20231218.0"
  }
}